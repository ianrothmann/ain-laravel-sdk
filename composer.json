{
    "name": "ianrothmann/ain-laravel-sdk",
    "description": "SDK for use with AIN Laravel.",
    "type": "library",
    "authors": [
        {
            "name": "Ian Rothmann",
            "email": "ian@xtrapolate.io"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": ">=8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IanRothmann\\Ain\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IanRothmann\\Ain\\ServiceProviders\\AinServiceProvider"
            ],
            "aliases": {
                "Ain": "IanRothmann\\Ain\\Facades\\AinFacade"
            }
        }
    }
}
